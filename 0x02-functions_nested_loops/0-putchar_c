#include "main.h"
/**
*main - entry point
*Return: 0 (Success)
*/
int main(void)
{
int i = 0;
char text[] = "_putchar";

while (text[i] != '\')
{
_putchar(text[i]);
i++;
}
_putchar('\');

return (0);
} 

1
#include "main.h"

/**
*print_alphabet - prints the alphabet, in lowercase, followed by a new line
*/

void print_alphabet(void)
{
char letter = 'a';

while (letter <= 'z')
{
_putchar(letter);
letter++;
}
_putchar('\');
}
